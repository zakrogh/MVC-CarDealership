@{
  Layout = "_Layout";
}
@using CarDealership.Models;
<h1>Cars:</h1>
@if (Model.Count == 0)
{
  <p>There are no items in the list.</p>
}
<ul>
  @foreach (Car car in Model)
  {
    <li>Make/Model: @car.MakeModel</li>
    <ul>
      <li>Price: @car.Price</li>
      <li>Miles: @car.Miles</li>
    </ul>
  }
</ul>
